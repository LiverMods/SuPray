local speedDesertBindable = Instance.new('BindableFunction')
function speedDesertBindable.OnInvoke(state)
	if state == '✅' then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/LiverMods/SuPray/main/Gajw"))()
	end
end
	game.StarterGui:SetCore('SendNotification', {
		Title = 'load the script',
		Text = 'Want to Load Script. This was created to prevent loading 2 Scripts at once',
		Duration = math.huge,
		Callback = speedDesertBindable,
		Button1 = '✅',
		Button2 = '❌'
	})
